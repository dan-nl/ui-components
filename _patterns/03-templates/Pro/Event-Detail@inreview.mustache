				</div>


			</div>


			<div class="section speakers">
				<h2>Speakers</h2>

				<ul class="people-list">
				{{#listItems.six}}<li>{{> molecules-person-block-small }}</li>{{/listItems.six}}
				</ul>
			</div>

			<div class="section files">
				<h2>Files</h2>
				{{> molecules-file-list-moredata }}
			</div>



			{{> molecules-related-images(title: "You might also like:") }}



		</div>
		</div><!--

	 --><div class="l-sidebar">
	 		<div class="inner">

				{{> molecules-help-contact(title: "Need Help?") }}
